    "components": {
        "schemas": {
            "DownloadResponse": {
                "properties": {
                    "model_id": {
                        "type": "integer",
                        "title": "Model Id"
                    },
                    "version_id": {
                        "type": "integer",
                        "title": "Version Id"
                    },
                    "model_dir": {
                        "type": "string",
                        "title": "Model Dir"
                    },
                    "model_type": {
                        "$ref": "#/components/schemas/ModelType"
                    }
                },
                "type": "object",
                "required": [
                    "model_id",
                    "version_id",
                    "model_dir",
                    "model_type"
                ],
                "title": "DownloadResponse"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "ModelInfo": {
                "properties": {
                    "model_id": {
                        "type": "integer",
                        "title": "Model Id"
                    },
                    "version_id": {
                        "type": "integer",
                        "title": "Version Id"
                    },
                    "model_dir": {
                        "type": "string",
                        "title": "Model Dir"
                    },
                    "filename": {
                        "type": "string",
                        "title": "Filename"
                    },
                    "model_type": {
                        "$ref": "#/components/schemas/ModelType"
                    }
                },
                "type": "object",
                "required": [
                    "model_id",
                    "version_id",
                    "model_dir",
                    "filename",
                    "model_type"
                ],
                "title": "ModelInfo"
            },
            "ModelType": {
                "type": "string",
                "enum": [
                    "lora",
                    "locon",
                    "vae",
                    "checkpoint",
                    "textualinversion"
                ],
                "title": "ModelType"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
